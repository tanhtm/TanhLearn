<Startup-Code startupcode="#---------------------------------------------#\012with(plottools):\012with(plots):\012#---------------------------------------------#\012RandPoints := proc(n,range_x,range_y)\012\009\012\009local P,roll, i, x, y:\012\009P:= vector(n): d:= 1:\012\009roll_x := rand(range_x);\012\009roll_y := rand(range_y);\012\009for i from 1 to n do:\012\009\009x:= roll_x(): y:= roll_y():\012\009\009P[d] := [x,y]: d:= d+1:\012\009end do:\012\009return convert(P,list):\012end proc:\012#---------------------------------------------#\012isCCW := proc(a,b,c) #a -&gt; b -&gt; c c\303\263 \304\221i ng\306\260\341\273\243c chi\341\273\201u kim \304\221\341\273\223ng h\341\273\223 kh\303\264ng ? \012\009#counterclockwise\012\009local d;\012\009d := a[1]*(b[2] - c[2]) + b[1]*(c[2] - a[2]) + c[1]*(a[2]-b[2]):\012\009if d &gt; 0 then return true; else return false; fi:\012end proc:\012#---------------------------------------------#\012ConvexHull := proc(P)\012\009local res,up,down,nUp,nDown,nPo,i,j,Points:\012\009Points := {op(P)}; #Chuy\341\273\203n v\341\273\201 t\341\272\255p set \304\221\341\273\203 s\341\272\257p x\341\272\277p, lo\341\272\241i b\341\273\217 l\341\272\267p\012\009nUp := 0: nDown := 0: nPo := nops(Points):\012\009up := vector(nPo): down:= vector(nPo):\012\009for i from 1 to nPo do:\012\009\009while nUp &gt;= 2 do:\012\009\009\009if isCCW(up[nUp-1],up[nUp],Points[i]) then nUp := nUp - 1;\012\009\009\009else break; fi;\012\009\009od:\012\009\009nUp := nUp + 1; up[nUp] := Points[i]:\012\009od:\012\009for i from nPo by -1 to 1 do:\012\009\009while nDown &gt;= 2 do:\012\009\009\009if isCCW(down[nDown-1],down[nDown],Points[i]) then nDown := nDown - 1:\012\009\009\009else break: fi:\012\009\009od:\012\009\009nDown := nDown + 1: down[nDown]:= Points[i]:\012\009od:\012\009res := vector(nUp + nDown - 2):\012\009for i from 1 to nUp do: res[i] := up[i]: od:\012\009for j from 2 to nDown-1 do: res[j - 1 + nUp]:= down[j]: od:\012\009return convert(res,list):\012end proc:\012#---------------------------------------------#\012myPlot := (p,listPlots) -&gt;  listPlots[trunc(p)];\012#---------------------------------------------#\012AniConvexHull := proc(P)\012\009local res, up, down, nUp, nDown, nPo, nPl, i, j, Points, p, l, aniUp, aniDown:\012\009Points := {op(P)}: #Chuy\341\273\203n v\341\273\201 t\341\272\255p set \304\221\341\273\203 s\341\272\257p x\341\272\277p, lo\341\272\241i b\341\273\217 l\341\272\267p\012\009nPo := nops(Points):\012\009if nPo &gt; 50 then print(&quot;L\341\273\227i: Ch\341\273\211 kh\341\272\243 d\341\273\245ng v\341\273\233i s\341\273\221 l\306\260\341\273\243ng nh\341\273\217 h\306\241n 50 \304\221i\341\273\203m&quot;): return: fi:\012\009up := vector(nPo): down:= vector(nPo):\012\009Plots := vector(5*nPo):\012\009nPl := 0:\012\009p := pointplot([Points[1],Points[nPo]],axes = none, color = red,symbolsize = 30, symbol = diamond):\012\009bg := display(pointplot(P,axes = none,symbolsize = 15),p):\012\009Plots[1] := p: nPl := 1:\012\009#-------Up---------#\012\009up[1] := Points[1]: nUp:= 1:\012\009for i from 2 to nPo do:\012\009\009p := display(p, line(up[nUp], Points[i], color = red, thickness= 3));\012\009\009nPl := nPl + 1: Plots[nPl] := p:\012\009\009while nUp &gt;= 2 do:\012\009\009\009if isCCW(up[nUp-1],up[nUp],Points[i]) then\012\009\009\009\009l := line(up[nUp],Points[i],color = gray, thickness= 3);\012\009\009\009\009p := display(p,l): nPl := nPl + 1: Plots[nPl] := p:\012\009\009\009\009l := line(up[nUp-1],up[nUp],color = gray, thickness= 3);\012\009\009\009\009p := display(p,l): nPl := nPl + 1: Plots[nPl] := p:\012\009\009\009\009l := line(up[nUp-1],Points[i],color = red,thickness= 3);\012\009\009\009\009p := display(p,l): nPl := nPl + 1: Plots[nPl] := p:\012\009\009\009\009nUp := nUp - 1:\012\009\009\009else break: fi:\012\009\009od:\012\009\009nUp := nUp + 1: up[nUp] := Points[i]:\012\009od:\012\009aniUp := animate(myPlot,[t,Plots],t=1..nPl,background=bg, frames = nPl, title=&quot;Convex Hull - Up&quot;);\012\009print(aniUp);\012\009#-------Down---------#\012\009p := pointplot([Points[1],Points[nPo]],axes = none, color = red,symbolsize = 30, symbol = diamond):\012\009Plots[1] := p: nPl := 1:\012\009down[1] := Points[nPo]: nDown := 1:\012\009for i from nPo - 1 by -1 to 1 do:\012\009\009p := display(p, line(down[nDown], Points[i], color = red, thickness= 3));\012\009\009nPl := nPl + 1: Plots[nPl] := p:\012\009\009while nDown &gt;= 2 do:\012\009\009\009if isCCW(down[nDown-1],down[nDown],Points[i]) then \012\009\009\009\009l := line(down[nDown],Points[i],color = gray, thickness= 3);\012\009\009\009\009p := display(p,l): nPl := nPl + 1: Plots[nPl] := p:\012\009\009\009\009l := line(down[nDown-1],down[nDown],color = gray, thickness= 3);\012\009\009\009\009p := display(p,l): nPl := nPl + 1: Plots[nPl] := p:\012\009\009\009\009l := line(down[nDown-1],Points[i],color = red,thickness= 3);\012\009\009\009\009p := display(p,l): nPl := nPl + 1: Plots[nPl] := p:\012\009\009\009\009nDown := nDown - 1:\012\009\009\009else break: fi:\012\009\009od:\012\009\009nDown := nDown + 1: down[nDown]:= Points[i]:\012\009od:\012\009aniDown := animate(myPlot,[t,Plots],t=1..nPl,background=bg, frames = nPl,title=&quot;Convex Hull - Down&quot;);\012\009print(aniDown);\012\009res := vector(nUp + nDown - 2):\012\009for i from 1 to nUp do: res[i] := up[i]: od:\012\009for j from 2 to nDown-1 do: res[j - 1 + nUp]:= down[j]: od:\012\009poly := polygon(convert(res,list),linestyle = solid, color = &quot;white&quot;,thickness= 3);\012\009print(display(poly,bg));\012end:\012#---------------------------------------------#"/>
